{
	"version": 2,
	"public": false,
	"builds": [{ "src": "app.py", "use": "@vercel/python" }],
	"routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ]
}